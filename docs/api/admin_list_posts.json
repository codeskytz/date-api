{
  "title": "List Posts",
  "method": "GET",
  "endpoint": "/api/v1/admin/posts",
  "auth": true,
  "description": "Get paginated list of all posts with filtering options",
  "request": {
    "headers": {
      "Authorization": "Bearer admin-token-xxx"
    },
    "query": {
      "page": 1,
      "limit": 20,
      "sort_by": "created_at",
      "order": "desc"
    }
  },
  "response": {
    "status": 200,
    "body": {
      "status": "success",
      "data": [],
      "pagination": {
        "total": 0,
        "page": 1,
        "limit": 20,
        "pages": 0
      }
    }
  }
}
